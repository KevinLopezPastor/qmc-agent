# ===========================================
# QMC Agent - Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Credenciales QMC
QMC_URL=https://your-qmc-server/qmc/tasks
QMC_USERNAME=your_username
QMC_PASSWORD=your_password

# API Groq (get your key at https://console.groq.com)
GROQ_API_KEY=gsk_your_api_key_here

# Configuraci√≥n del Agente
MAX_RETRIES=3
HEADLESS=True
TIMEOUT_MS=30000

# NPrinting Credentials
NPRINTING_URL=https://your-nprinting-server:4993/#/tasks/executions
NPRINTING_EMAIL=your_email@company.com
NPRINTING_PASSWORD=your_password
